{
  "plugins": [
    "plugins/markdown",
    "plugin-jsdoc-ts.cjs"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["src-transpiler"],
    "includePattern": ".+\\.(js(doc|x)?|mjs)$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/@playcanvas/jsdoc-template"
  }
}
